{
  "name": "react-complete-monorepo-template",
  "private": true,
  "packageManager": "pnpm@9.15.4",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "type-check": "turbo type-check",
    "preinstall": "node scripts/preinstall-guard.mjs",
    "dev:web": "node scripts/run.mjs dev web",
    "dev:desktop": "node scripts/run.mjs dev desktop",
    "dev:mobile": "pnpm --filter \"./apps/mobile/*\" start",
    "dev:api": "node scripts/run.mjs dev api",
    "dev:cron": "node scripts/run.mjs dev cron",
    "dev:all": "node scripts/run.mjs dev all",

    "build:web": "node scripts/run.mjs build web",
    "build:desktop": "node scripts/run.mjs build desktop",
    "build:packages": "pnpm --filter ./packages/* build",
    "build:all": "node scripts/run.mjs build all",

    "lint:all": "node scripts/run.mjs lint all",
    "type-check:all": "node scripts/run.mjs type-check all",
    "create:web": "node scripts/create-web.mjs",
    "create:desktop": "node scripts/create-desktop.mjs",
    "create:cron-worker": "node scripts/create-cron-worker.mjs",
    "create:mobile": "node scripts/create-mobile.mjs",
    "check:doctor": "node scripts/doctor.mjs",
    "create:worker": "node scripts/create-worker.mjs"
  },
  "devDependencies": {
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.0",
    "turbo": "2.8.0",
    "typescript": "5.9.3"
  },
  "pnpm": {
    "overrides": {
      "tsup>rollup": "3.29.4",
      "react": "19.1.0",
      "react-dom": "19.1.0",
      "@types/react": "19.1.0",
      "@types/react-dom": "19.1.0"
    }
  }
}